{"ast":null,"code":"import FieldAddTask from \"@/components/Todo/FieldAddTask.vue\";\nexport default {\n  name: \"Home\",\n  components: {\n    \"field-add-task\": FieldAddTask\n  }\n};","map":{"version":3,"mappings":"AAsCA;AAEA;EACAA,YADA;EAEAC;IACA;EADA;AAFA","names":["name","components"],"sourceRoot":"src/views","sources":["TodoView.vue"],"sourcesContent":["<template>\n  <div>\n    <field-add-task />\n    <v-list v-if=\"$store.state.tasks.length\" class=\"pt-0\" flat>\n      <div v-for=\"task in $store.state.tasks\" :key=\"task.id\">\n        <v-list-item\n          @click=\"$store.commit('doneTask', task.id)\"\n          :class=\"{ ' blue lighten-5': task.done }\"\n        >\n          <template v-slot:default>\n            <v-list-item-action>\n              <v-checkbox :input-value=\"task.done\"></v-checkbox>\n            </v-list-item-action>\n\n            <v-list-item-content>\n              <v-list-item-title\n                :class=\"{ 'text-decoration-line-through': task.done }\"\n                >{{ task.title }}</v-list-item-title\n              >\n            </v-list-item-content>\n            <v-list-item-action>\n              <v-btn @click.stop=\"$store.commit('deleteTask', task.id)\" icon>\n                <v-icon color=\"primary lighten-1\">mdi-delete</v-icon>\n              </v-btn>\n            </v-list-item-action>\n          </template>\n        </v-list-item>\n        <v-divider></v-divider>\n      </div>\n    </v-list>\n    <div v-else class=\"no-tasks\">\n      <v-icon size=\"100\" color=\"primary \"> mdi-check </v-icon>\n      <div class=\"text-h5 primary--text\">No tasks</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport FieldAddTask from \"@/components/Todo/FieldAddTask.vue\";\n\nexport default {\n  name: \"Home\",\n  components: {\n    \"field-add-task\": FieldAddTask,\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.no-tasks\n  position: absolute\n  left: 50%\n  top: 50%\n  transform: translate(-50%,-50%)\n  opacity: 0.5\n</style>\n"]},"metadata":{},"sourceType":"module"}